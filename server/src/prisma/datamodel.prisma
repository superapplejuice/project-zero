type User {
  id: ID! @id
  username: String! @unique
  email: String! @unique
  password: String!
  createdAt: DateTime! @createdAt
}

type Item {
  id: ID! @id
  name: String!
  description: String!
  price: Int!
  image_1: String!
  image_2: String
  image_3: String
  image_4: String
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}